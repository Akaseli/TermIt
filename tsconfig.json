{
  "compilerOptions": {
    "target": "ES2021",
    "module": "CommonJS",
    "moduleResolution": "node",
    "lib": [
      "dom",
      "ES2021"
    ],
    "jsx": "react-jsx",
    "noEmit": true ,
    "isolatedModules": true,
    "esModuleInterop": true,
    "strict": true ,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  },
  "include": [
    "./frontend/src"
  ]
}